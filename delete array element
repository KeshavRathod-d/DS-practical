#include<stdio.h> 
int main(){ 
    int a[1000],n,j,position; 
    printf("enter the number of array elements"); 
    scanf("%d",&n); 
 
    for(int i=0;i<n;i++){ 
        printf("enter the elements %d",i+1); 
        scanf("%d",&a[i]); 
    } 
    int pos; 
    printf("enter the position of element to where you want to delete element"); 
    scanf("%d",&position); 
if 
    (position>=n+1) 
{ 
    printf("invalid position"); 
} 
else 
   for(j=position-1;j<n-1;j++) 
    {a[j]=a[j+1]; 
        printf("Resultant array is:\n"); 
    } 
    for(j=0;j<n-1;j++) 
    { 
    printf("%d\n",a[j]); 
    } 
return  0; 
}
